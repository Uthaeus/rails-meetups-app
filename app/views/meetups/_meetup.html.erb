<div id="<%= dom_id meetup %>">
  <p>
    <strong>Title:</strong>
    <%= meetup.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= meetup.description %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= meetup.time %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= meetup.date %>
