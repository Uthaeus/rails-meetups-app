<div class='meetups-container'>
  <h1 class="meetups-title">Meetups</h1>
  <%= link_to "New meetup", new_meetup_path, class: "btn btn-primary" if logged_in?(:site_admin, :user) %>

  <div id="meetups" class="row-cols-3 g-4">
    <% @meetups.each do |meetup| %>
      <%= render meetup %>
    <% end %>
  </div>

  
</div>
